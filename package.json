		"prepublishOnly": "npm run build",
		"test": "tsdx test"
	},
	"dependencies": {
		"saxes": "^5.0.1"
	},
	"devDependencies": {
		"@types/globby": "^9.1.0",
		"@types/jest": "^24.0.15",
		"@types/sax": "^1.2.0",
		"globby": "^9.0.0",
		"husky": "^3.0.0",
		"jest": "^24.8.0",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1",
		"rimraf": "^2.6.3",
		"semantic-release": "^15.13.3",
		"ts-jest": "^24.0.2",
		"tsdx": "^0.7.2",
		"tslib": "^1.10.0",
		"typescript": "^3.5.3"
	},
	"prettier": {
		"printWidth": 72,
		"semi": true,
		"singleQuote": true,
